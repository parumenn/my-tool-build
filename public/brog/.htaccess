
DirectoryIndex index.html

# Disable MultiViews (can interfere with routing)
<IfModule mod_rewrite.c>
    Options -MultiViews
</IfModule>

# Cache Settings
<IfModule mod_expires.c>
  ExpiresActive On
  ExpiresDefault "access plus 1 month"
  ExpiresByType text/html "access plus 0 seconds"
</IfModule>
